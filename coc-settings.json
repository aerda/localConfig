{
  "coc-autoselect": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "<-",
  "diagnostic.virtualTextLineNr": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.floatEnable": true,
  "coc.preferences.jumps": {
    "goes": "both"
  },
  "coc.source.jumps.keymaps": {
    "<CR>": "jumpInsert",
    "k": "jumpPrev",
    "j": "jumpNext"
  }
}
